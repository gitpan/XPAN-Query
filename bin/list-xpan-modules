#!/usr/bin/env perl

use 5.010;
use strict;
use warnings;

use Perinci::CmdLine 1.04;

our $DATE = '2014-06-05'; # DATE
our $VERSION = '0.02'; # VERSION

Perinci::CmdLine->new(url => "/XPAN/Query/list_xpan_modules")->run;

1;
